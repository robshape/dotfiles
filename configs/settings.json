{
  "breadcrumbs.enabled": false,
  "cSpell.flagWords": [],
  "cSpell.showStatus": false,
  "debug.internalConsoleOptions": "openOnSessionStart",
  "debug.openDebug": "openOnSessionStart",
  "debug.showInStatusBar": "always",
  "debug.toolBarLocation": "docked",
  "diffEditor.ignoreTrimWhitespace": false,
  "docker.showStartPage": false,
  "editor.autoClosingQuotes": "beforeWhitespace",
  "editor.autoIndent": "keep",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.find.seedSearchStringFromSelection": false,
  "editor.formatOnSave": true,
  "editor.lineNumbers": "relative",
  "editor.links": false,
  "editor.renderWhitespace": "boundary",
  "editor.scrollBeyondLastLine": false,
  "editor.snippetSuggestions": "none",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "editor.wrappingIndent": "indent",
  "eslint.lintTask.enable": true,
  "eslint.run": "onSave",
  "explorer.autoReveal": false,
  "extensions.ignoreRecommendations": true,
  "files.hotExit": "off",
  "files.insertFinalNewline": true,
  "files.restoreUndoStack": false,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.confirmSync": false,
  "git.promptToSaveFilesBeforeCommit": "staged",
  "javascript.preferences.quoteStyle": "single",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "liveshare.allowInvites": "none",
  "liveshare.anonymousGuestApproval": "reject",
  "liveshare.autoShareServers": false,
  "liveshare.guestApprovalRequired": true,
  "liveshare.increasedGuestLimit": false,
  "liveshare.openSharedServers": false,
  "liveshare.presence": false,
  "liveshare.shareExternalFiles": false,
  "liveshare.showInStatusBar": "never",
  "markdownlint.run": "onSave",
  "npm-intellisense.importLinebreak": ";\n",
  "npm-intellisense.showBuildInLibs": true,
  "prettier.requireConfig": true,
  "prettier.useEditorConfig": false,
  "react-native.ios.runArguments.simulator": ["--simulator", "iPhone X"],
  "rust.build_on_save": true,
  "search.followSymlinks": false,
  "solidity.packageDefaultDependenciesContractsDirectory": "",
  "stylelint.reportNeedlessDisables": true,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.rightClickBehavior": "default",
  "typescript.preferences.quoteStyle": "single",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vim.easymotion": true,
  "vim.insertModeKeyBindings": [
    {
      "after": ["<esc>"],
      "before": ["i", "i"]
    }
  ],
  "vim.mouseSelectionGoesIntoVisualMode": false,
  "vim.normalModeKeyBindings": [
    {
      "after": ["<leader>", "<leader>", "<leader>", "b", "d", "w"],
      "before": ["<space>"]
    }
  ],
  "vim.useSystemClipboard": true,
  "window.nativeTabs": true,
  "window.newWindowDimensions": "maximized",
  "window.openFilesInNewWindow": "on",
  "window.titleBarStyle": "native",
  "workbench.colorTheme": "Gloom",
  "workbench.commandPalette.history": 0,
  "workbench.editor.openPositioning": "last",
  "workbench.fontAliasing": "auto",
  "workbench.panel.defaultLocation": "right",
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "zenMode.centerLayout": false,
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust",
    "editor.formatOnSave": true
  }
}
