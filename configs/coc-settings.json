{
  "bashIde.backgroundAnalysisMaxFiles": 0,
  "bashIde.shellcheckPath": "",
  "coc.preferences.formatOnSave": true,
  "colors.enable": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
  },
  "eslint.run": "onSave",
  "git.realtimeGutters": false,
  "go.goplsOptions": {
    "gofumpt": true,
    "staticcheck": true
  },
  "markdownlint.onChange": false,
  "markdownlint.onOpen": false,
  "prettier.onlyUseLocalVersion": true,
  "prettier.requireConfig": true,
  "stylelintplus.autoFixOnSave": true,
  "stylelintplus.cssInJs": true,
  "stylelintplus.validateOnSave": true,
  "stylelintplus.validateOnType": false,
  "svelte.enable-ts-plugin": false,
  "tailwindCSS.headwind.runOnSave": true
}
